/* miniprogram_npm/vi-radio/index.wxss */

@font-face {font-family: "iconfont";
  src: url('iconfont.eot?t=1540890758252'); /* IE9*/
  src: url('iconfont.eot?t=1540890758252#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
  url('iconfont.ttf?t=1540890758252') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('iconfont.svg?t=1540890758252#iconfont') format('svg'); /* iOS 4.1- */
  font-weight: normal;
  font-style: normal;
}


.icon{
  font-weight: 500;
  font-style: normal;
  font-family: 'iconfont';
}

.icon-circleo:before { /* 圆圈*/
  content: "\eb95"; 
}

.icon-icfeedbacksuccessblack:before { /*success*/
  content: "\e651"; 
}

.radio{
  display: inline-block;
}
.radio__circle{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  border: 1px solid #ccc;
  border-radius: 50%;
  text-align: center;
  vertical-align: middle;
}
.radio>text{
  margin-left: 20rpx;
}
.radio .icon{
  font-size: 40rpx;
}